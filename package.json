    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "@types/express": "^4.0.36",
    "express": "^4.15.4",
    "firebase-admin": "^5.2.1",
    "firebase-functions": "^0.6.2",
    "firebase-tools": "^3.9.2",
    "html-webpack-plugin": "^2.30.1",
    "rollup": "^0.47.6",
    "rollup-plugin-commonjs": "^8.1.0",
